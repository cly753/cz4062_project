<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sg.com.ntu.cz4062.group9.contact"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="18" />

    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        <activity
            android:name="sg.com.ntu.cz4062.group9.contact.MyContacts"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="sg.com.ntu.cz4062.group9.contact.service.MyService"
            android:exported="true" >
            <intent-filter>
                <action android:name="sg.com.ntu.cz4062.group9.contact.CONTACT_SENDER" />
            </intent-filter>
        </service>
    </application>

</manifest>